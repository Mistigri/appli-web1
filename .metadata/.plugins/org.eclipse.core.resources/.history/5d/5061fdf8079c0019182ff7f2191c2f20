
import java.util.Date;

@XmlRootElement
@XmlAccessorType(XmlAccessType.FIELD)


public class Compte {
	private Long code;
	private double solde;
	private Date dateCreation;
	
	public Compte() {
	}
	
	public Compte(Long code, double solde, Date dateCreation) {
		this.code = code;
		this.solde=solde;
		this.dateCreation=dateCreation;
	}


}

